{
  "$schema": "https://json.schemastore.org/package.json",
  "version": "0.0.1",
  "name": "ccode",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "typecheck": "tsgo --noEmit",
    "lint": "tsgo --noEmit",
    "format": "prettier --check .",
    "format:write": "prettier --write .",
    "test": "bun test",
    "test:tui": "bun test test/unit/tui test/integration/tui test/e2e/tui/critical",
    "test:tui:unit": "bun test test/unit/tui",
    "test:tui:integration": "bun test test/integration/tui",
    "test:tui:e2e": "bun test test/e2e/tui",
    "test:tui:coverage": "bun test --coverage test/unit/tui test/integration/tui",
    "test:tui:visual": "bun test test/e2e/tui/visual",
    "test:tui:perf": "bun test test/performance/tui",
    "test:tui:a11y": "bun test test/accessibility/tui",
    "test:tui:all": "bun test test/unit/tui test/integration/tui test/e2e/tui test/performance/tui test/accessibility/tui",
    "test:coverage": "bun test --coverage",
    "test:coverage:report": "bun test --coverage --coverage-dir=./coverage",
    "test:verify": "bun test/verify-coverage.ts",
    "build": "bun run script/build.ts",
    "build:single": "bun run script/build.ts --single",
    "build:baseline": "bun run script/build.ts --single --baseline",
    "build:skip-install": "bun run script/build.ts --skip-install",
    "dev": "bun run --conditions=browser ./src/index.ts"
  },
  "bin": {
    "ccode": "./bin/ccode"
  },
  "exports": {
    "./*": "./src/*.ts"
  },
  "devDependencies": {
    "@babel/core": "7.28.4",
    "@parcel/watcher-darwin-arm64": "2.5.1",
    "@parcel/watcher-darwin-x64": "2.5.1",
